<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
    </dict>
    <key>Label</key>
    <string>com.ribose.cryptoded</string>
    <key>ProgramArguments</key>
    <array>
      <string>/opt/cryptode/bin/cryptoded</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>/tmp</string>
    <key>StandardErrorPath</key>
    <string>/var/log/cryptoded/cryptoded.log</string>
    <key>StandardOutPath</key>
    <string>/var/log/cryptoded/cryptoded.log</string>
  </dict>
</plist>
